        <SteadyState Duration="240" Power="0.4"/>
        <SteadyState Duration="60" Power="0.9"/>
        <IntervalsT Repeat="3" OnDuration="5" OffDuration="115" OnPower="1.7" OffPower="0.9"/>
        <IntervalsT Repeat="1" OnDuration="5" OffDuration="55" OnPower="1.7" OffPower="0.9"/>
        <SteadyState Duration="240" Power="0.4"/>
        <SteadyState Duration="60" Power="0.94"/>
        <IntervalsT Repeat="3" OnDuration="5" OffDuration="115" OnPower="1.5" OffPower="0.94"/>
