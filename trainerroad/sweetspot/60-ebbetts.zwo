        <SteadyState Duration="60" Power="0.65"/>
        <SteadyState Duration="30" Power="0.5"/>
        <SteadyState Duration="60" Power="0.75"/>
        <SteadyState Duration="30" Power="0.5"/>
        <SteadyState Duration="60" Power="0.85"/>
        <SteadyState Duration="30" Power="0.5"/>
        <SteadyState Duration="60" Power="0.95"/>
        <SteadyState Duration="180" Power="0.4"/>
        <SteadyState Duration="60" Power="0.92"/>
        <IntervalsT Repeat="3" OnDuration="5" OffDuration="115" OnPower="1.6" OffPower="0.92"/>
