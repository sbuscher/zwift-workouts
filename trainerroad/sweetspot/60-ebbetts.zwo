        <SteadyState Duration="240" Power="0.4"/>
        <SteadyState Duration="60" Power="0.88"/>
        <IntervalsT Repeat="3" OnDuration="5" OffDuration="115" OnPower="1.8" OffPower="0.88"/>
        <IntervalsT Repeat="1" OnDuration="5" OffDuration="55" OnPower="1.8" OffPower="0.88"/>
        <Cooldown Duration="300" PowerLow="0.4" PowerHigh="0.3"/>
    </workout>
</workout_file>
